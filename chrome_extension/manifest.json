{
  "manifest_version": 2,
  "name": "TransVid",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.3.1.min.js", "PhotoExtractor.js"]
    }
  ],
  "browser_action" : {
      "default_popup" : "popup.html"
  },
  "permissions": [ "tabs" ]
}